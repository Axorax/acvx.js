{
  "compilerOptions": {
    "target": "ES2019",
    "lib": ["ES2019"],
    "module": "ESNext",
    "moduleResolution": "Node",
    "outDir": "./dist",
    "esModuleInterop": true,
    "strict": true,
    "skipLibCheck": true,
    "noEmitOnError": true,
    "removeComments": true,
    "sourceMap": false,
    "inlineSourceMap": false,
    "declaration": true,
    "declarationMap": false,
    "resolveJsonModule": true
  },
  "include": ["*.ts"],
  "exclude": ["node_modules"]
}
